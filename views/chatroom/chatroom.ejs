<!DOCTYPE html>
<head>

    <link href="https://fonts.googleapis.com/css?family=Open+Sans:700,600" rel="stylesheet" type="text/css">
    <link rel="stylesheet" type="text/css" href="css/style.css">
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.0/jquery.min.js" type="text/javascript"></script>
    
    <!-- <script src="/https://cdn.socket.io/socket.io.js"></script> -->
    <!-- <script src="partials/index.js"></script> -->
    
    
</head>
<html>
<body>
    <!-- <img src="https://www.w3schools.com/html/pulpitrock.jpg" alt="FUCK"> -->
    <div class="box">
        <div>Advanced Spirit Animal AI</div>
        <div class ="chatBox" id="chat-text">
            <% for(var i=0; i < locals.allData.length; i++) { %>
                <div id = <%=locals.allData[i].name%> class="chatCell"><%=locals.allData[i].name%>'s spirit animal is a <%=locals.allData[i].address%></div>
            <% } %>
        </div>
        <form id="chat-form">
            <label for="name-input">Name</label>
            <input id="name-input" type="text" autocomplete="off" id="send-button">
            <button type="submit" id="send-button">Send</button>
        </form>
    </div>
    <!-- <script src="/jquery/dist/jquery.js"></script> -->
    <script src="/socket.io/socket.io.js"></script>
    <script defer src="javascript/chatroom.js"></script>
</body>
</html>
      